# Seni Indah Gypsum Frontend Environment Variables
# Copy this file to .env.local and update the values

# Backend API URL
NEXT_PUBLIC_API_URL=https://api.seniindah.co.id
NEXT_PUBLIC_API_TIMEOUT=10000

NEXT_PUBLIC_PROJECT_NAME="Seni Indah Gypsum & Interior"

# Encryption key (must match backend ENCRYPTION_KEY)
NEXT_PUBLIC_ENCRYPTION_KEY=40f146bf902f69223f1087ac3bcf97e87581d5b07c59e5e38738feb456ad7e23


# Optional: API version
NEXT_PUBLIC_API_VERSION=v1.0

# Optional: Environment
NEXT_PUBLIC_ENVIRONMENT=development

# Example values for development:
# NEXT_PUBLIC_API_URL=http://localhost:8787
# NEXT_PUBLIC_ENCRYPTION_KEY=a1b2c3d4e5f6789012345678901234567890abcdef1234567890abcdef123456

# Example values for production:
# NEXT_PUBLIC_API_URL=https://seni-indah-gypsum-backend.your-subdomain.workers.dev
# NEXT_PUBLIC_ENCRYPTION_KEY=your-production-encryption-key-here